{
	"presets": ["react", "es2015"],
	"plugins": [
		"transform-object-rest-spread",
		"transform-runtime",
		"tailcall-optimization",
		"transform-class-properties",
		"react-hot-loader/babel"
	],
	"env": {
		"development": {
			"plugins": [
				"transform-object-rest-spread",
				"tailcall-optimization",
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-catch-errors",
						"imports": ["react", "redbox-react", "redboxOptions"]
					}]
				}]
			]
		}
	}
}
