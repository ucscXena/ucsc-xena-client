{
  "name": "ucsc-xena-client",
  "version": "2.100.0-alpha.0",
  "description": "UCSC Xena Client. Functional genomics visualizations.",
  "homepage": "https://xena.ucsc.edu",
  "main": "index.js",
  "scripts": {
    "test": "webpack serve --config webpack.test.js --mode development --port 8081",
    "pretest:headless": "webpack --config webpack.karma.js",
    "test:headless": "karma start --single-run --browsers ChromeHeadless karma.conf.js build/index.html",
    "lint": "eslint --no-color js/*.js js/*/*.js test/*.js",
    "eslint": "eslint",
    "clean": "rimraf build/*",
    "clean:dist": "rimraf dist/*",
    "start": "npm run start:hot",
    "start:cold": "webpack serve --mode development",
    "start:hot": "webpack serve --config webpack.hot.js --mode development",
    "start:prod": "webpack serve --config webpack.prod.js --mode production",
    "build": "webpack",
    "build:prod": "webpack --config webpack.prod.js --mode production",
    "build:analyze": "npm run -s build:prod -- --profile --json > stats.json",
    "prebuild": "npm run clean",
    "dist": "npm run clean:dist && mkdir -p dist && cp -r js/* dist && NODE_ENV=production babel -d dist js && webpack --config webpack.dist.js",
    "prepublishOnly": "npm run dist"
  },
  "files": [
    "NOTICE",
    "LICENSE",
    "dist"
  ],
  "engines": {
    "npm": ">=8.0.0"
  },
  "author": "Brian Craft",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
