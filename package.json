{
  "name": "ucsc-xena-client",
  "version": "2.100.0-alpha.0",
  "description": "UCSC Xena Client. Functional genomics visualizations.",
  "homepage": "https://xena.ucsc.edu",
  "main": "index.js",
  "scripts": {
    "test": "webpack-dev-server --config webpack.test.js --devtool eval --port 8081",
    "pretest:headless": "webpack --config webpack.test.js --devtool eval",
    "test:headless": "karma start --single-run --browsers ChromeHeadless karma.conf.js build/index.html",
    "lint": "eslint --no-color js/*.js js/*/*.js test/*.js",
    "eslint": "eslint",
    "clean": "rimraf build/*",
    "clean:dist": "rimraf dist/*",
    "start": "npm run start:hot",
    "start:cold": "webpack-dev-server --devtool eval --progress --colors --history-api-fallback",
    "start:hot": "webpack-dev-server --config webpack.hot.js --devtool eval-source-map --progress --colors --history-api-fallback",
    "start:prod": "webpack-dev-server --config webpack.prod.js --devtool source-map --progress --colors --history-api-fallback",
    "build": "webpack",
    "build:prod": "npm run build -- --config webpack.prod.js --devtool source-map",
    "build:analyze": "npm run -s build:prod -- --profile --json > stats.json",
    "prebuild": "npm run clean",
    "dist": "npm run clean:dist && mkdir -p dist && cp -r js/* dist && NODE_ENV=production babel -d dist js && webpack --config webpack.dist.js",
    "prepublishOnly": "npm run dist",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run -s cypress/integration/heatmap-spec.js",
    "cypress:gen": "mocha --require babel-core/register cypress/generators/suite.js"
  },
  "files": [
    "NOTICE",
    "LICENSE",
    "dist"
  ],
  "author": "Brian Craft",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "arraybuffer-loader": "^1.0.8",
    "assert": "^1.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-tailcall-optimization": "^2.0.0",
    "chai": "^3.5.0",
    "css-loader": "^4.2.2",
    "csslint": "^0.10.0",
    "cypress": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-ucsc-xena-client": "file:eslint-rules",
    "file-loader": "^6.0.0",
    "ghooks": "^1.2.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^4.3.0",
    "js-sha1": "^0.6.0",
    "jsverify": "^0.7.1",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^2.0.1",
    "mini-css-extract-plugin": "^0.10.0",
    "mocha": "^8.1.1",
    "mocha-loader": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.1",
    "postcss-for": "^2.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-modules-values": "^1.2.2",
    "raw-loader": "^4.0.1",
    "react-hot-loader": "^4.12.21",
    "react-json-tree": "^0.5.1",
    "react-overlays": "^5.1.1",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "request": "^2.88.0",
    "rimraf": "^2.3.2",
    "schema-shorthand": "^3.1.0",
    "semver": "^5.1.0",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.2.1",
    "transform-loader": "^0.2.4",
    "val-loader": "^2.1.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.2",
    "xss": "^1.0.6"
  },
  "dependencies": {
    "@luma.gl/debug": "^8.5.21",
    "@luma.gl/webgl": "^8.5.12",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@statoscope/webpack-plugin": "^5.26.2",
    "blob-stream": "^0.1.3",
    "brfs": "^1.4.3",
    "classnames": "2.2.5",
    "core-js": "^3.6.5",
    "deck.gl": "^8.6.7",
    "ehmutable": "^1.4.0",
    "fast-deep-equal": "git+ssh://git@github.com/ucscXena/fast-deep-equal.git",
    "highcharts": "^6.0.7",
    "iconv-lite": "^0.6.3",
    "jStat": "1.5.3",
    "just-diff": "^6.0.2",
    "linear-algebra": "3.1.1",
    "lz-string": "^1.4.4",
    "minhash": "0.0.9",
    "normalize.css": "7.0.0",
    "pdfkit": "^0.11.0",
    "platform": "^1.3.5",
    "postcss": "^6.0.1",
    "postcss-cssnext": "^2.11.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.4.2",
    "react-iframe": "^1.8.0",
    "react-material-responsive-grid": "1.2.0",
    "react-resizable": "1.8.0",
    "react-router-dom": "^5.3.0",
    "reselect": "^2.0.1",
    "rxjs": "^5.2.0",
    "science": "^1.9.3",
    "showdown": "1.3.0",
    "static-interval-tree": "^1.3.0",
    "svg-to-pdfkit": "^0.1.8",
    "ucsc-xena-wasm": "^1.12.0",
    "underscore": "^1.10.2",
    "xlsx": "^0.16.9"
  }
}
