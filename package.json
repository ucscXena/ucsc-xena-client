    "imports-loader": "^0.6.3",
    "jshint": "^2.6.3",
    "jsverify": "^0.6.0-alpha.3",
    "jsx-loader": "^0.12.2",
    "mocha": "^2.2.1",
    "mocha-loader": "^0.7.1",
    "napa": "^1.2.0",
    "rimraf": "^2.3.2",
    "style-loader": "^0.8.3",
