    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.7.0",
    "redux-devtools-dock-monitor": "^1.2.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "request": "^2.88.0",
    "rimraf": "^2.3.2",
    "schema-shorthand": "^3.1.0",
